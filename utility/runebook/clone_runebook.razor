# Unset any previous serial set
unsetvar! 'myrunebook'
unsetvar! 'mynewrunebook'

# Setup runebook serial
overhead 'Select runebook to clone'
setvar! 'myrunebook'

# Setup runebook serial
overhead 'Select new runebook'
setvar! 'mynewrunebook'

######################
while mana < 32
    if skill 'Meditation' > 1
        hotkey 'Meditation'
    endif
endwhile

# Location 1-1
overhead 'Location 1-1'
dclick 'myrunebook'
waitforgump 1551740969
gumpresponse 5
wait 3000
hotkey 'Mark'
waitfortarget
targettype 7956 'backpack' 2882
wait 1000
if findtype 7956 'backpack' 0 as 'targetrune'
    lift 'targetrune' 1
endif
drop 'mynewrunebook' -1 -1 0

while mana < 32
    if skill 'Meditation' > 1
        hotkey 'Meditation'
    endif
endwhile

