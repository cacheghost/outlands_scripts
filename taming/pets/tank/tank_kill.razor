@unsetvar 'allkill_target'
overhead '[Tank Kill]' 44
@setvar! 'allkill_target'

# Kill
if skill 'Herding' >= 1
    if varexist 'tank1'
        menu 'tank1' 2
        wait 100
        target 'allkill_target'
    endif
else
    if varexist 'tank1'
        menu 'tank1' 1
        wait 100
        target 'allkill_target'
    endif
endif
script main