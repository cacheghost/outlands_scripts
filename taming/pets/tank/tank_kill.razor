@unsetvar 'allkill_target'
overhead '[Tank Kill]' 44
@setvar! 'allkill_target'

# Kill
if skill 'Herding' >= 1
    if varexist 'tank1'
        if dead 'tank1'
            # Ignore
        else
            menu 'tank1' 2
            wait 100
            target 'allkill_target'
        endif
    endif
else
    if varexist 'tank1'
        if dead 'tank1'
            # Ignore
        else
            menu 'tank1' 1
            wait 100
            target 'allkill_target'
        endif
    endif
endif
script main